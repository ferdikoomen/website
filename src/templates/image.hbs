<figure class="image" role="presentation" aria-hidden="true" tabindex="-1">
	<picture>
		<source media="(min-width: 1840px)" srcset="/static/images/{{{ filename }}}-ultrawide.webp 1x, /static/images/{{{ filename }}}-ultrawide@2x.webp 2x" type="image/webp"/>
		<source media="(min-width: 1840px)" srcset="/static/images/{{{ filename }}}-ultrawide.jpg 1x, /static/images/{{{ filename }}}-ultrawide@2x.jpg 2x" type="image/jpeg"/>
		<source media="(min-width: 1560px)" srcset="/static/images/{{{ filename }}}-superwide.webp 1x, /static/images/{{{ filename }}}-superwide@2x.webp 2x" type="image/webp"/>
		<source media="(min-width: 1560px)" srcset="/static/images/{{{ filename }}}-superwide.jpg 1x, /static/images/{{{ filename }}}-superwide@2x.jpg 2x" type="image/jpeg"/>
		<source media="(min-width: 1280px)" srcset="/static/images/{{{ filename }}}-wide.webp 1x, /static/images/{{{ filename }}}-wide@2x.webp 2x" type="image/webp"/>
		<source media="(min-width: 1280px)" srcset="/static/images/{{{ filename }}}-wide.jpg 1x, /static/images/{{{ filename }}}-wide@2x.jpg 2x" type="image/jpeg"/>
		<source media="(min-width: 1000px)" srcset="/static/images/{{{ filename }}}-desktop.webp 1x, /static/images/{{{ filename }}}-desktop@2x.webp 2x" type="image/webp"/>
		<source media="(min-width: 1000px)" srcset="/static/images/{{{ filename }}}-desktop.jpg 1x, /static/images/{{{ filename }}}-desktop@2x.jpg 2x" type="image/jpeg"/>
		<source media="(min-width: 480px)" srcset="/static/images/{{{ filename }}}-tablet.webp 1x, /static/images/{{{ filename }}}-tablet@2x.webp 2x" type="image/webp"/>
		<source media="(min-width: 480px)" srcset="/static/images/{{{ filename }}}-tablet.jpg 1x, /static/images/{{{ filename }}}-tablet@2x.jpg 2x" type="image/jpeg"/>
		<source media="(max-width: 479px)" srcset="/static/images/{{{ filename }}}-mobile.webp 1x, /static/images/{{{ filename }}}-mobile@2x.webp 2x" type="image/webp"/>
		<source media="(max-width: 479px)" srcset="/static/images/{{{ filename }}}-mobile.jpg 1x, /static/images/{{{ filename }}}-mobile@2x.jpg 2x" type="image/jpeg"/>
		<img src="/static/images/{{{ filename }}}-ultrawide.jpg" alt="{{{ alt }}}"/>
	</picture>
</figure>
